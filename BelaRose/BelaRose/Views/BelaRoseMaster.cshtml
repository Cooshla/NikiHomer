@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.BelaRoseMaster>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <!--[if IE]>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="@CurrentPage.Description">
    <meta name="author" content="@CurrentPage.Author">
    <!-- Page title -->
    <title>@CurrentPage.Title</title>
    <!--[if lt IE 9]>
    <script src="js/respond.js"></script>
    <![endif]-->
    <!-- Bootstrap Core CSS -->
    <link href="~/css/bootstrap.css" rel="stylesheet" type="text/css">
    <!-- Icon fonts -->
    <link href="~/fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/fonts/flaticons/flaticon.css" rel="stylesheet" type="text/css">
    <link href="~/fonts/glyphicons/glyphicons.css" rel="stylesheet" type="text/css">
    <!-- Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Lato:400,700,700italic' rel='stylesheet' type='text/css'>
    <!-- Style Css -->
    <link href="~/css/style.css" rel="stylesheet">
    <!-- Color Skin Css -->
    <link href="~/css/skins/passion.css" rel="stylesheet">
    <!-- Plugins CSS -->
    <link href="~/css/plugins.css" rel="stylesheet">
    <!-- LayerSlider stylesheet -->
    <link rel="stylesheet" href="~/layerslider/css/layerslider.css">
    <!-- Favicons-->
    <link rel="apple-touch-icon" sizes="72x72" href="~/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/apple-touch-icon-114x114.png">
    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon">
</head>
<body>

    <div class="top-bar navbar-fixed-top">
        <div class="container">
            <div class="row">
                <div class="top-logo page-scroll col-lg-2  col-md-2 col-sm-2">
                    <!-- Preloader Logo -->
                    <a href="#page-top">
                        <img src="@Umbraco.Media(CurrentPage.LogoDark).Url" class="center-block img-responsive" alt="" />
                    </a>
                </div>
                <div class="col-lg-8 col-md-8  col-sm-8 pull-right">
                    <!-- Start Social Links -->
                    <ul class="social-list hidden-xs">
                        <li><a title="Facebook" href="@CurrentPage.FacebookLink"><i class="fa fa-facebook"></i></a></li>
                        <li><a title="Twitter" href="@CurrentPage.TwitterLink"><i class="fa fa-twitter"></i></a></li>
                        <li><a title="Tumblr" href="@CurrentPage.TumblrLink"><i class="fa fa-tumblr"></i></a></li>
                        <li><a title="Instagram" href="@CurrentPage.InstagramLink"><i class="fa fa-instagram"></i></a></li>
                    </ul>
                    <!-- /End Social Links -->
                    <!-- Start Contact Info -->
                    <ul class="contact-details hidden-sm hidden-xs">
                        <li><i class="fa fa-map-marker"></i>@CurrentPage.Location</li>
                        <li><i class="fa fa-envelope"></i>@CurrentPage.Email</li>
                    </ul>
                    <!-- End Contact Info -->
                </div>
            </div>
        </div>
    </div>
    <!-- End Top bar -->
    <!-- Preloader -->
    <div id="preloader">
        <div class="col-md-5 col-centered logo-load">
            <!-- Preloader Logo -->
            <img src="@Umbraco.Media(CurrentPage.LoaderLogo).Url" class="center-block img-responsive" alt="" />
        </div>
        <div class="preloader">
            <span></span>
            <span></span>
            <span></span>
            <span></span>
            <span></span>
        </div>
    </div>
    <!-- Navbar -->
    <div id="wrapper">
        <div class="container">
            <!-- menu Button -->
            <button type="button" class="hamburger is-closed" data-toggle="offcanvas">
                <span class="hamb-top"></span>
                <span class="hamb-middle"></span>
                <span class="hamb-bottom"></span>
            </button>
        </div>
        <div class="overlay"></div>
        <!-- Sidebar -->
        <nav class="navbar navbar-inverse navbar-fixed-top page-scroll" id="sidebar-wrapper">
            <!-- Sidebar Logo -->
            <div class="sidebar-brand">
                <a href="#page-top">
                    <img src="@Umbraco.Media(CurrentPage.Logo).Url" class="item img-responsive" alt="" />
                </a>
            </div>
            <!-- Sidebar menu -->
            <ul class="nav sidebar-nav">
                <li class="item"><a href="#page-top">Home</a></li>
                <li class="item"><a href="#services">Services</a></li>
                <li class="item"><a href="#about">About Us</a></li>
                <li class="item"><a href="#team">Our Team</a></li>
                <li class="item"><a href="#prices">Prices</a></li>
                <li class="item"><a href="#gallery">Gallery</a></li>
                @if (!CurrentPage.IsBlogInActive)
                {
                <li class="item"><a href="~/blog">Blog</a></li>
                }
                <li class="item"><a href="#contact">Contact</a></li>
            </ul>
        </nav>
        <!-- /#sidebar-wrapper -->
        <!-- Page Content -->
        <div id="page-content-wrapper">
          


            @RenderBody()

            <!-- Footer -->
            <footer class="dark-section">
                <div class="container-fluid">
                    <!-- Bottom Logo -->
                    <div class="col-lg-4 col-lg-offset-2 col-sm-5 res-margin">
                        <div class="page-scroll">
                            <a href="#page-top">
                                <img src="@Umbraco.Media(CurrentPage.Logo).Url" alt="" class="img-responsive center-block">
                            </a>
                        </div>
                        <!-- social-icons -->
                        <div class="social-media">
                            <a href="@CurrentPage.TwitterLink" title="Twitter"><i class="fa fa-twitter"></i></a>
                            <a href="@CurrentPage.FacebookLink" title="Facebook"><i class="fa fa-facebook"></i></a>
                            <a href="@CurrentPage.TumblrLink" title="Tumblr"><i class="fa fa-tumblr"></i></a>
                            <a href="@CurrentPage.InstagramLink" title="Instagram"><i class="fa fa-instagram"></i></a>

                        </div>
                    </div>
                    <!-- /col-lg-4 -->
                    <div class="col-lg-4 col-sm-6">
                        <h6>Opening Hours:</h6>
                        <!-- Table-->
                        @CurrentPage.OpeningHours
                    </div>
                </div>
                <!-- / container -->
                <hr>
                <p>Copyright © 2016 - Powered by <a href="http://cooshla.com">Cooshla</a></p>
                <!-- /container -->
                <!-- Go To Top Link -->
                <div class="page-scroll hidden-sm hidden-xs">
                    <a href="#page-top" class="back-to-top"><i class="fa fa-angle-up"></i></a>
                </div>
            </footer>
            <!-- /footer ends -->
        </div>
        <!-- /page-content-wrapper -->
    </div>
    <!-- /wrapper -->
    <!-- Core JavaScript Files -->
    <script src="~/scripts/jquery.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
    <!-- Google maps -->
    <script src="https://maps.googleapis.com/maps/api/js?v=3"></script>
    <!-- Main Js -->
    <script src="~/scripts/main.js"></script>
    <!--Mail Chimp validator -->
    <script src="~/scripts/mc-validate.js"></script>
    <!--Other Plugins -->
    <script src="~/scripts/plugins.js"></script>
    <!-- Prefix free CSS -->
    <script src="~/scripts/prefixfree.js"></script>
    <!-- GreenSock -->
    <script src="~/layerslider/js/greensock.js" type="text/javascript"></script>
    <!-- LayerSlider script files -->
    <script src="~/layerslider/js/layerslider.transitions.js" type="text/javascript"></script>
    <script src="~/layerslider/js/layerslider.kreaturamedia.jquery.js" type="text/javascript"></script>
    <!--Date Picker -->
    <script src="~/scripts/datetimepicker.js"></script>
</body>
</html>