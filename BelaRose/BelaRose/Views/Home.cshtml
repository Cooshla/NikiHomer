@inherits Vega.USiteBuilder.UmbracoTemplatePageBase<BelaRose.DocumentType.Home>
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <!--[if IE]>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="@Model.MetaDescription">
    <meta name="keywords" content="@Model.MetaKeywords">
    <meta name="author" content="@Model.Author">
    <!-- Page title -->
    <title>@Model.MetaTitle</title>
    <!--[if lt IE 9]>
    <script src="js/respond.js"></script>
    <![endif]-->
    <!-- Bootstrap Core CSS -->
    <link href="~/css/bootstrap.css" rel="stylesheet" type="text/css">
    <!-- Icon fonts -->
    <link href="~/fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/fonts/flaticons/flaticon.css" rel="stylesheet" type="text/css">
    <link href="~/fonts/glyphicons/glyphicons.css" rel="stylesheet" type="text/css">
    <!-- Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Lato:400,700,700italic' rel='stylesheet' type='text/css'>
    <!-- Style Css -->
    <link href="~/css/style.css" rel="stylesheet">
    <!-- Color Skin Css -->
    <link href="~/css/skins/passion.css" rel="stylesheet">
    <!-- Plugins CSS -->
    <link href="~/css/plugins.css" rel="stylesheet">
    <!-- LayerSlider stylesheet -->
    <link rel="stylesheet" href="~/layerslider/css/layerslider.css">
    <!-- Favicons-->
    <link rel="apple-touch-icon" sizes="72x72" href="~/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/apple-touch-icon-114x114.png">
    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon">
</head>
<body>

    <div class="top-bar navbar-fixed-top">
        <div class="container">
            <div class="row">
                <div class="top-logo page-scroll col-lg-2  col-md-2 col-sm-2">
                    <!-- Preloader Logo -->
                    <a href="#page-top">
                        <img src="@Umbraco.Media(Model.LogoDark).Url" class="center-block img-responsive" alt="" />
                    </a>
                </div>
                <div class="col-lg-8 col-md-8  col-sm-8 pull-right">
                    <!-- Start Social Links -->
                    <ul class="social-list hidden-xs">
                        <li><a title="Facebook" href="@Model.FaceBook"><i class="fa fa-facebook"></i></a></li>
                        <li><a title="Twitter" href="@Model.Twitter"><i class="fa fa-twitter"></i></a></li>
                        <li><a title="Instagram" href="@Model.Instagram"><i class="fa fa-instagram"></i></a></li>
                    </ul>
                    <!-- /End Social Links -->
                    <!-- Start Contact Info -->
                    <ul class="contact-details hidden-sm hidden-xs">
                        <li><i class="fa fa-map-marker"></i>@Model.InfoAddress</li>
                        <li><i class="fa fa-envelope"></i>@Model.InfoEmail</li>
                    </ul>
                    <!-- End Contact Info -->
                </div>
            </div>
        </div>
    </div>
    <!-- End Top bar -->
    <!-- Preloader -->
    <div id="preloader">
        <div class="col-md-5 col-centered logo-load">
            <!-- Preloader Logo -->
            <img src="@Umbraco.Media(Model.LoaderLogo).Url" class="center-block img-responsive" alt="" />
        </div>
        <div class="preloader">
            <span></span>
            <span></span>
            <span></span>
            <span></span>
            <span></span>
        </div>
    </div>
    <!-- Navbar -->
    <div id="wrapper">
        <div class="container">
            <!-- menu Button -->
            <button type="button" class="hamburger is-closed" data-toggle="offcanvas">
                <span class="hamb-top"></span>
                <span class="hamb-middle"></span>
                <span class="hamb-bottom"></span>
            </button>
        </div>
        <div class="overlay"></div>
        <!-- Sidebar -->
        <nav class="navbar navbar-inverse navbar-fixed-top page-scroll" id="sidebar-wrapper">
            <!-- Sidebar Logo -->
            <div class="sidebar-brand">
                <a href="#page-top">
                    <img src="@Umbraco.Media(Model.Logo).Url" class="item img-responsive" alt="" />
                </a>
            </div>
            <!-- Sidebar menu -->
            <ul class="nav sidebar-nav">
                <li class="item"><a href="#page-top">Home</a></li>
                <li class="item"><a href="#services">Services</a></li>
                <li class="item"><a href="#about">About Us</a></li>
                <li class="item"><a href="#team">Our Team</a></li>
                <li class="item"><a href="#prices">Prices</a></li>
                <li class="item"><a href="#gallery">Gallery</a></li>
               
                <li class="item"><a href="#contact">Contact</a></li>
            </ul>
        </nav>
        <!-- /#sidebar-wrapper -->
        <!-- Page Content -->
        <div id="page-content-wrapper">


            <!-- Video Header -->
            <div class="header-video" style="background-image: url('@Umbraco.Media(Model.BackgroundImage).Url')">
                <video autoplay muted loop>
                    <source src="~/videos/index_video.mp4" type="video/mp4">
                    <source src="~/videos/index_video.ogg" type="video/ogg">
                    <!-- Video Cover image by CSS -->
                </video>
                <div class="video-wrapper">
                    <!-- Video Title -->
                    <h1 class="video-title">@Model.H1Tag</h1>
                </div>
            </div>
            <!-- /video header ends-->
            <!-- Call to Action Section -->
            <section id="call-to-action" class="color-section small-section">
                <div class="container wow fadeInUp">
                    <div class="col-lg-7 col-lg-offset-4 col-md-7 col-md-offset-4">
                        <div class="text-center">
                            <h4>@Model.CTATitle</h4>
                            <p>@Model.CTAText</p>
                            <!-- Button -->
                            <div class="page-scroll">
                                <a class="btn" href="#contact">@Model.CTAButtonText</a>
                            </div>
                        </div>
                        <!-- /text-center -->
                    </div>
                    <!-- /col-lg-7-->
                </div>
                <!-- /container-->
            </section>
            <!-- Section call-to-action Ends-->
            <!-- Section Services -->
            <section id="services">
                <div class="container">
                    <div class="col-md-6 col-lg-6">
                        <!-- Section heading -->
                        <div class="section-heading">
                            <h2 class="text-left">@Model.ServicesTitle</h2>
                        </div>
                        <!-- main text -->
                        <h3>@Model.ServicesSubTitle</h3>
                        <p>
                            @Model.ServicesContent
                        </p>
                    </div>
                    <!-- /col-md-6-->
                </div>
                <!-- /container -->
                <!-- Services Accordion Stars-->
                <ul class="container-fluid accordion-group" id="accordion">

                    @{
                        if (Model.ServicePanels != null)
                        {
                            var ServiceList = Model.ServicePanels.ToString().Split(new string[] { "," }, StringSplitOptions.RemoveEmptyEntries);
                            var ServiceCollection = Umbraco.Content(ServiceList);

                            foreach (var item in ServiceCollection)
                            {
                            <!-- Panel 1 -->
                            <!-- Accordion item1-->
                                if (item.IsFirst)
                                {
                                    @:<li class="out">
                                        }
                                        else
                                        {
                                        @:<li>
                                        }

                                        <div class="accordion-overlay"></div>
                                        <h4>@item.Title</h4>
                                        <div class="hidden-xs">
                                            <article>
                                                <h6>@item.SubHeading</h6>
                                                <p>@item.Content</p>
                                            </article>
                                        </div>
                                        @:</li>
                                    }
                                    }

                                    }

                                </ul>
                <!-- /Accordion ends -->
                            </section>


            <!-- /Section Services ends -->
            <!-- Section About -->
                                    <section id="about">
                                        <div class="container">
                                            <!-- Row -->
                                            <div class="row margin-about">
                                                <div class="col-lg-7 col-sm-12">
                                                    <!-- Section heading -->
                                                    <div class="section-heading">
                                                        <h2 class="text-left">@Model.AboutTitle</h2>
                                                    </div>
                                                    <h3>@Model.AboutSubTitle</h3>
                                                    <p>
                                                        @Model.AboutContent
                                                    </p>
                                                </div>
                                                <!-- Diamond, image though CSS -->
                                                <div class="wow fadeInUp">
                                                    <div class="diamond">
                                                        <div class="square">
                                                            <div class="pic">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!-- wow fade -->
                                            </div>
                                            <!-- /row -->
                                            <!-- row stars -->
                                            <div class="row">
                                                <div class="col-md-7">
                                                    <!-- Image Carousel -->
                                                    <div id="owl-about" class="owl-carousel">
                                                        @{
                                                            if (Model.AboutCarousel1 != null)
                                                            {
                                                                var BlogList = Model.AboutCarousel1.ToString().Split(new string[] { "," }, StringSplitOptions.RemoveEmptyEntries);
                                                                var BlogCollection = Umbraco.Content(BlogList);

                                                                foreach (var item in BlogCollection)
                                                                {
                                                                    <img src="@Umbraco.Media(item).Url" alt="">
                                                                }
                                                            }
                                                        }
                                                    </div>
                                                </div>
                                                <!-- /col-md-7 -->
                                                <div class="col-md-5">
                                                    <!-- ACCORDION ROW -->
                                                    <div class="panel-group" id="accordion2">
                                                        @{
                                                            if (Model.AboutPanels != null)
                                                            {
                                                                var PanelList = Model.AboutPanels.ToString().Split(new string[] { "," }, StringSplitOptions.RemoveEmptyEntries);
                                                                var PanelCollection = Umbraco.Content(PanelList);

                                                                foreach (var item in PanelCollection)
                                                                {
                                                                    string idstring = "collapse" + item.Id;
                                                                    string classString = item.IsFirst ? "panel-collapse collapse in" : "panel-collapse collapse";
                            <!-- Panel 1 -->
                                                                    <div class="panel">
                                                                        <div class="panel-heading">
                                                                            <h6 class="panel-title">
                                                                                <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#@idstring">@item.Title</a>
                                                                            </h6>
                                                                        </div>

                                                                        <div id="@idstring" class="@classString">
                                                                            <div class="panel-body">
                                                                                <p>@item.Content</p>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                }
                                                            }

                                                        }
                                                    </div>
                                                    <!-- /panel-group -->
                                                </div>
                                                <!-- /col-md-5 -->
                                            </div>
                                            <!--/row -->
                                        </div>
                                        <!--/container-->
                                    </section>
            <!--/ Section About ends -->
            <!-- Section Prices -->
                                                            <section id="prices">
                                                                <div class="container">
                                                                    <!-- Section Heading -->
                                                                    <div class="section-heading">
                                                                        <h2>@Model.PricesTitle</h2>
                                                                    </div>

                                                                    <!-- Price tabs Start -->
                                                                    <div class="tabbable">
                                                                        <div class="tab-content col-lg-10 col-centered">

                                                                            <div class="tab-pane active in fade" id="tab2">
                                                                                <div class="row">
                                                                                    <!-- Menu: Skin & Body -->
                                                                                    <div class="col-md-12">
                                                                                        <!-- image -->
                                                                                        <img class="img-responsive" src="img/price2.jpg" alt="">
                                                                                        <h3>@Model.ListTitle</h3>
                                                                                    </div>
                                                                                    <div class="col-md-6">
                                                                                        <!-- menu body -->
                                                                                        <div class="menu-body">
                                                                                            <div class="menu-section">



                                                                                                @{
                                                                                                    if (Model.PriceList != null)
                                                                                                    {
                                                                                                        var PriceList = Model.PriceList.ToString().Split(new string[] { "," }, StringSplitOptions.RemoveEmptyEntries);
                                                                                                        var PriceCollection = Umbraco.Content(PriceList);

                                                                                                        foreach (var item in PriceCollection)
                                                                                                        {

                                    <!-- Item starts -->
                                                                                                            <div class="menu-item">
                                                                                                                <div class="menu-item-name">
                                                                                                                    @item.Title
                                                                                                                </div>
                                                                                                                <div class="menu-item-price">
                                                                                                                    @item.SubHeading
                                                                                                                </div>
                                                                                                                <div class="menu-item-description">
                                                                                                                    <p>@item.Content</p>
                                                                                                                </div>
                                                                                                            </div>
                                    <!-- Item ends -->


                                                                                                        }
                                                                                                    }
                                                                                                }

                                                                                            </div>
                                                                                            <!--/ menu section -->
                                                                                        </div>
                                                                                        <!-- / menu body -->
                                                                                    </div>
                                                                                    <!-- /col-md-6 -->
                                                                                    <!-- second column -->
                                                                                    <div class="col-md-6">
                                                                                        <div class="menu-body">
                                                                                            <div class="menu-section">
                                                                                                @{
                                                                                                    if (Model.PriceList2 != null)
                                                                                                    {
                                                                                                        var PriceList2 = Model.PriceList2.ToString().Split(new string[] { "," }, StringSplitOptions.RemoveEmptyEntries);
                                                                                                        var PriceCollection2 = Umbraco.Content(PriceList2);

                                                                                                        foreach (var item in PriceCollection2)
                                                                                                        {

                                    <!-- Item starts -->
                                                                                                            <div class="menu-item">
                                                                                                                <div class="menu-item-name">
                                                                                                                    @item.Title
                                                                                                                </div>
                                                                                                                <div class="menu-item-price">
                                                                                                                    @item.SubHeading
                                                                                                                </div>
                                                                                                                <div class="menu-item-description">
                                                                                                                    <p>@item.Content</p>
                                                                                                                </div>
                                                                                                            </div>
                                    <!-- Item ends -->


                                                                                                        }
                                                                                                    }
                                                                                                }
                                                                                            </div>
                                                                                            <!--/ menu section -->
                                                                                        </div>
                                                                                        <!-- / menu body -->
                                                                                    </div>
                                                                                    <!-- /col-md-6 -->
                                                                                </div>
                                                                                <!-- /.row -->
                                                                            </div>

                                                                        </div>
                                                                        <!--tab-content-->
                                                                    </div>
                                                                    <!--tababble-->
                                                                </div>
                                                                <!-- /container -->
                                                            </section>
            <!-- /Section Prices ends -->
            <!-- Section Gallery -->
                                                                                                    <section id="gallery" class="gray-section">
                                                                                                        <div class="container-fluid">
                                                                                                            <!-- Section heading -->
                                                                                                            <div class="section-heading">
                                                                                                                <h2>@Model.GalleryTitle</h2>
                                                                                                            </div>

                                                                                                            <!-- Gallery -->
                                                                                                            <div class="row">
                                                                                                                <div class="col-md-12 wow fadeInUp">
                                                                                                                    <div id="lightbox">

                                                                                                                        @{
                                                                                                                            if (Model.Images != null)
                                                                                                                            {
                                                                                                                                var ImageList = Model.Images.ToString().Split(new string[] { "," }, StringSplitOptions.RemoveEmptyEntries);
                                                                                                                                var ImageCollection = Umbraco.Content(ImageList);

                                                                                                                                foreach (var item in ImageCollection)
                                                                                                                                {
                                                                                                                                    <div class="bodywellbeing col-lg-4 col-sm-6 col-md-6">
                                                                                                                                        <div class="isotope-item">
                                                                                                                                            <div class="gallery-thumb">
                                                                                                                                                <img class="img-responsive" src="@Umbraco.Media(item).Url" alt="">
                                                                                                                                                <a href="@Umbraco.Media(item).Url" data-gal="prettyPhoto[gallery]" title="You can add caption to pictures.">
                                                                                                                                                    <span class="overlay-mask"></span>
                                                                                                                                                </a>
                                                                                                                                            </div>
                                                                                                                                        </div>
                                                                                                                                    </div>
                                                                                                                                }
                                                                                                                            }
                                                                                                                        }
                                                                                                                    </div>
                                                                                                                    <!-- /lightbox-->
                                                                                                                </div>
                                                                                                                <!-- /col-md-12-->
                                                                                                            </div>
                                                                                                            <!-- /row -->
                                                                                                        </div>
                                                                                                        <!-- /container -->
                                                                                                    </section>
            <!-- Section Gallery ends -->
            <!-- Section Contact -->
                                                                                                                            <section id="contact">
                                                                                                                                <div class="container">
                                                                                                                                    <!-- Section heading -->
                                                                                                                                    <div class="section-heading">
                                                                                                                                        <h2>Contact us</h2>
                                                                                                                                    </div>
                                                                                                                                    <!-- Contact -->
                                                                                                                                    <div class="col-lg-9 col-centered text-center">
                                                                                                                                        <h3>Make an Appointment</h3>
                                                                                                                                        <p>@Model.ContactBlurb</p>
                                                                                                                                        <!-- Form Starts -->
                                                                                                                                        <div id="contact_form">
                                                                                                                                            <!-- form results -->
                                                                                                                                            <div id="contact_results"></div>
                                                                                                                                            <!-- Form Starts -->
                                                                                                                                            <div class="form-group">
                                                                                                                                                <div class="col-lg-6">
                                                                                                                                                    <input type="text" name="name" class="col-lg-6 form-control input-field" placeholder="Name" required="">
                                                                                                                                                </div>
                                                                                                                                                <div class="col-lg-6">
                                                                                                                                                    <input type="email" name="email" class="form-control input-field" placeholder="Email ID" required="">
                                                                                                                                                </div>
                                                                                                                                                <div class="col-lg-6">
                                                                                                                                                    <input type="text" name="service" class="form-control input-field" placeholder="Service" required="">
                                                                                                                                                </div>
                                                                                                                                                <div class="col-lg-6">
                                                                                                                                                    <div class="input-group date" id="datetimepicker1">
                                                                                                                                                        <input type="text" name="date" class="form-control input-field" placeholder="Date and Time" required="" />
                                                                                                                                                        <span class="input-group-addon">
                                                                                                                                                            <span class="fa fa-calendar"></span>
                                                                                                                                                        </span>
                                                                                                                                                    </div>
                                                                                                                                                </div>
                                                                                                                                                <div class="col-lg-12">
                                                                                                                                                    <textarea name="message" id="message" class="textarea-field form-control" rows="4" placeholder="Enter your message" required=""></textarea>
                                                                                                                                                    <button type="submit" id="submit_btn" value="Submit" class="btn center-block">Send message</button>
                                                                                                                                                </div>
                                                                                                                                            </div>
                                                                                                                                            <!-- /Form group-->
                                                                                                                                        </div>
                                                                                                                                        <!--/Contact form -->
                                                                                                                                    </div>
                                                                                                                                    <!--/col-lg-9 -->
                                                                                                                                </div>
                                                                                                                                <!-- /container-->
                                                                                                                            </section>
            <!--Section Contact ends -->
            <!--Section Info -->
                                                                                                                            <section id="info" class="small-section">
                                                                                                                                <div class="container">
                                                                                                                                    <div class="col-lg-6 col-md-6 col-centered text-center">
                                                                                                                                        <div class="well wow fadeInUp">
                                                                                                                                            <h5>Information</h5>
                                                                                                                                            <!-- contact info -->
                                                                                                                                            <div class="contact-info">
                                                                                                                                                <p><i class="fa fa-envelope"></i><a href="@Model.InfoEmail">@Model.InfoEmail</a></p>
                                                                                                                                                <p><i class="fa fa-phone margin-icon"></i>Call Us @Model.InfoPhone</p>
                                                                                                                                            </div>
                                                                                                                                            <!-- address info -->
                                                                                                                                            <p>We are located at @Model.InfoAddress.</p>
                                                                                                                                            <!-- Map -->
                                                                                                                                            <div id="map-canvas"></div>
                                                                                                                                            <!-- Newsletter -->
                                                                                                                                            <h5>our Newsletter</h5>
                                                                                                                                            <p>We will send updates once a week.</p>

                                                                                                                                            <!-- Newsletter Form -->
                                                                                                                                            <div id="mc_embed_signup">
                                                                                                                                                <!-- Your form adress in the line bellow -->
                                                                                                                                                <form action="//yourlist.us12.list-manage.com/subscribe/post?u=1111" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
                                                                                                                                                    <div id="mc_embed_signup_scroll">
                                                                                                                                                        <div class="mc-field-group">
                                                                                                                                                            <div class="input-group">
                                                                                                                                                                <input class="form-control input-lg required email" type="email" value="" name="EMAIL" placeholder="Your email here" id="mce-EMAIL">
                                                                                                                                                                <span class="input-group-btn">
                                                                                                                                                                    <button type="submit" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="btn">Subscribe</button>
                                                                                                                                                                </span>
                                                                                                                                                            </div>
                                                                                                                                                            <!-- Subscription results -->
                                                                                                                                                            <div id="mce-responses" class="mailchimp">
                                                                                                                                                                <div class="alert alert-danger response" id="mce-error-response"></div>
                                                                                                                                                                <div class="alert alert-success response" id="mce-success-response"></div>
                                                                                                                                                            </div>
                                                                                                                                                        </div>
                                                                                                                                                        <!-- /mc-fiel-group -->
                                                                                                                                                    </div>
                                                                                                                                                    <!-- /mc_embed_signup_scroll -->
                                                                                                                                                </form>
                                                                                                                                                <!-- /form ends -->
                                                                                                                                            </div>
                                                                                                                                            <!-- /mc_embed_signup -->
                                                                                                                                        </div>
                                                                                                                                        <!-- /well -->
                                                                                                                                    </div>
                                                                                                                                    <!-- /col-lg-6-->
                                                                                                                                </div>
                                                                                                                                <!--/container -->
                                                                                                                            </section>
            <!-- /Section Info ends -->


                                                                                                                            <!-- Footer -->
                                                                                                                            <footer class="dark-section">
                                                                                                                                <div class="container-fluid">
                                                                                                                                    <!-- Bottom Logo -->
                                                                                                                                    <div class="col-lg-4 col-lg-offset-2 col-sm-5 res-margin">
                                                                                                                                        <div class="page-scroll">
                                                                                                                                            <a href="#page-top">
                                                                                                                                                <img src="@Umbraco.Media(Model.Logo).Url" alt="" class="img-responsive center-block">
                                                                                                                                            </a>
                                                                                                                                        </div>
                                                                                                                                        <!-- social-icons -->
                                                                                                                                        <div class="social-media">
                                                                                                                                            <a href="@Model.Twitter" title="Twitter"><i class="fa fa-twitter"></i></a>
                                                                                                                                            <a href="@Model.FaceBook" title="Facebook"><i class="fa fa-facebook"></i></a>
                                                                                                                                            <a href="@Model.Instagram" title="Instagram"><i class="fa fa-instagram"></i></a>

                                                                                                                                        </div>
                                                                                                                                    </div>
                                                                                                                                    <!-- /col-lg-4 -->
                                                                                                                                    <div class="col-lg-4 col-sm-6">
                                                                                                                                        <h6>Opening Hours:</h6>
                                                                                                                                        <!-- Table-->
                                                                                                                                        @Html.Raw(Model.OpeningHours)
                                                                                                                                    </div>
                                                                                                                                </div>
                                                                                                                                <!-- / container -->
                                                                                                                                <hr>
                                                                                                                                <p>Copyright © 2016 - Powered by <a href="http://cooshla.com">Cooshla</a></p>
                                                                                                                                <!-- /container -->
                                                                                                                                <!-- Go To Top Link -->
                                                                                                                                <div class="page-scroll hidden-sm hidden-xs">
                                                                                                                                    <a href="#page-top" class="back-to-top"><i class="fa fa-angle-up"></i></a>
                                                                                                                                </div>
                                                                                                                            </footer>
            <!-- /footer ends -->
                                                                                                                        </div>
        <!-- /page-content-wrapper -->
                                                                                                                    </div>
    <!-- /wrapper -->
    <!-- Core JavaScript Files -->
                                                                                                                            <script src="~/scripts/jquery.min.js"></script>
                                                                                                                            <script src="~/scripts/bootstrap.min.js"></script>
    <!-- Google maps -->
                                                                                                                            <script src="https://maps.googleapis.com/maps/api/js?v=3"></script>
    <!-- Main Js -->
                                                                                                                            <script src="~/scripts/main.js"></script>
    <!--Mail Chimp validator -->
                                                                                                                            <script src="~/scripts/mc-validate.js"></script>
    <!--Other Plugins -->
                                                                                                                            <script src="~/scripts/plugins.js"></script>
    <!-- Prefix free CSS -->
                                                                                                                            <script src="~/scripts/prefixfree.js"></script>
    <!-- GreenSock -->
                                                                                                                            <script src="~/layerslider/js/greensock.js" type="text/javascript"></script>
    <!-- LayerSlider script files -->
                                                                                                                            <script src="~/layerslider/js/layerslider.transitions.js" type="text/javascript"></script>
                                                                                                                            <script src="~/layerslider/js/layerslider.kreaturamedia.jquery.js" type="text/javascript"></script>
